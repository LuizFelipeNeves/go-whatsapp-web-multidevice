{
  "name": "whatsapp-multi-platform",
  "version": "1.0.0",
  "description": "Plataforma escalável para gerenciar múltiplos números de WhatsApp utilizando containers Docker",
  "main": "api-gateway/src/server.js",
  "scripts": {
    "start": "node api-gateway/src/server.js",
    "dev": "nodemon api-gateway/src/server.js",
    "docker:build": "docker build -t whatsapp-instance whatsapp-container/",
    "docker:compose": "docker-compose up -d --build",
    "test": "jest",
    "logs": "docker-compose logs -f"
  },
  "keywords": ["whatsapp", "docker", "api", "scaling", "multidevice"],
  "author": "WPP Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dockerode": "^4.0.0",

    "p-queue": "^6.6.2",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "cron": "^3.1.6",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.4",
    "js-yaml": "^4.1.0",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/node": "^20.10.0"
